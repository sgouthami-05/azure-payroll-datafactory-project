{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-nycpayroll-gouthami"
		},
		"LS_ADLS_NYC_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_ADLS_NYC'"
		},
		"LS_AzureSQL_NYC_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'LS_AzureSQL_NYC'"
		},
		"LS_ADLS_NYC_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://storageaccount555.dfs.core.windows.net/"
		},
		"LS_AzureSQL_NYC_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "gouthami.database.windows.net"
		},
		"LS_AzureSQL_NYC_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "db-nyc-payroll"
		},
		"LS_AzureSQL_NYC_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "admin123"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-nycpayroll-gouthami_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_ADLS_NYC_accountKey": {
						"value": "[parameters('LS_ADLS_NYC_accountKey')]"
					},
					"LS_AzureSQL_NYC_password": {
						"value": "[parameters('LS_AzureSQL_NYC_password')]"
					},
					"LS_ADLS_NYC_properties_typeProperties_url": {
						"value": "[parameters('LS_ADLS_NYC_properties_typeProperties_url')]"
					},
					"LS_AzureSQL_NYC_properties_typeProperties_server": {
						"value": "[parameters('LS_AzureSQL_NYC_properties_typeProperties_server')]"
					},
					"LS_AzureSQL_NYC_properties_typeProperties_database": {
						"value": "[parameters('LS_AzureSQL_NYC_properties_typeProperties_database')]"
					},
					"LS_AzureSQL_NYC_properties_typeProperties_userName": {
						"value": "[parameters('LS_AzureSQL_NYC_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-nycpayroll-gouthami_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-nycpayroll-gouthami_ArmTemplate_0')]"
			]
		}
	]
}